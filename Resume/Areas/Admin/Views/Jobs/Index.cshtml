@model IEnumerable<Resume.Areas.Admin.Models.Job>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.StartDate)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.EndDate)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.CompanyName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.TittleEs)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.TittleEn)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.LocationEn)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.LocationEs)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.StartDate)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.EndDate)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CompanyName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.TittleEs)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.TittleEn)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.LocationEn)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.LocationEs)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Id">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Id">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Id">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
