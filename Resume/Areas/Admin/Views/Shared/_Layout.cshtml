<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - Resume</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" />
</head>
<body>
    <header>
        <nav class="navbar navbar-expand-sm navbar-toggleable-sm navbar-light bg-white border-bottom box-shadow mb-3">
            <div class="container">
                <a class="navbar-brand" asp-area="Admin" asp-controller="Home" asp-action="Index">Resume</a>
                <button class="navbar-toggler" type="button" data-toggle="collapse" data-target=".navbar-collapse" aria-controls="navbarSupportedContent"
                        aria-expanded="false" aria-label="Toggle navigation">
                    <span class="navbar-toggler-icon"></span>
                </button>
                <div class="navbar-collapse collapse d-sm-inline-flex flex-sm-row-reverse">
                    <partial name="_LoginPartial" />
                    <ul class="navbar-nav flex-grow-1">
                        <li class="nav-item">
                            <a class="nav-link text-dark" asp-area="Admin" asp-controller="Home" asp-action="Index">Home</a>
                        </li>
                    </ul>
                </div>
            </div>
        </nav>
    </header>
    <div class="container-fluid h-100">
        <div class="row h-100">
            <div class="col-3" id="green">
                <h4>Entidades</h4>
                <!-- Navigation links in sidebar-->
                <ul class="list-group">
                    <li class="list-group-item">
                        <a asp-area="Admin" asp-controller="CVModels" asp-action="Index">CVModels</a>
                    </li>
                    <li class="list-group-item">
                        <a asp-area="Admin" asp-controller="Jobs" asp-action="Index">Jobs</a>
                    </li>
                    <li class="list-group-item">
                        <a asp-area="Admin" asp-controller="JobResponsibilities" asp-action="Index">JobResponsibilities</a>
                    </li>
                    <li class="list-group-item">
                        <a asp-area="Admin" asp-controller="PersonalInfo" asp-action="Index">PersonalInfo</a>
                    </li>
                    <li class="list-group-item">
                        <a asp-area="Admin" asp-controller="CVModel_JobResp" asp-action="Index">CVModel_JobResp</a>
                    </li>
                    <li class="list-group-item">
                        <a asp-area="Admin" asp-controller="Skills" asp-action="Index">Skills</a>
                    </li>
                </ul>
            </div>
            <main role="main" class="pb-3 col-9">
                @RenderBody()
            </main>
        </div>
    </div>

    <footer class="border-top footer text-muted">
        <div class="container">
            &copy; 2022 - Resume - <a asp-area="" asp-controller="Home" asp-action="Privacy">Privacy</a>
        </div>
    </footer>
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    @RenderSection("Scripts", required: false)
</body>
</html>
